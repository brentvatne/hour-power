// https://pris.ly/d/prisma-schema

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id                     Int       @id @default(autoincrement())
  spotifyUserId          String    @unique
  spotifyToken           String
  spotifyRefreshToken    String
  spotifyTokenExpiration DateTime
}
